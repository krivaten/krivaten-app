{
  "name": "krivaten-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm --filter ./frontend dev\" \"pnpm --filter ./workers dev\"",
    "frontend": "pnpm --filter ./frontend dev",
    "workers": "pnpm --filter ./workers dev",
    "build": "pnpm --filter ./frontend build",
    "deploy:workers": "pnpm --filter ./workers run deploy",
    "typecheck": "pnpm --filter ./workers typecheck"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "packageManager": "pnpm@10.18.0+sha512.e804f889f1cecc40d572db084eec3e4881739f8dec69c0ff10d2d1beff9a4e309383ba27b5b750059d7f4c149535b6cd0d2cb1ed3aeb739239a4284a68f40cfa"
}
